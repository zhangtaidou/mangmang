<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.yijiayifeng.mapper.AccountMapper">
    <insert id="addAccount" parameterType="account">
       insert into account
        (admin_id,name,number,numbercopy,username,ctitys,ctityss)
        values (#{admin_id},#{name},#{number},#{numbercopy},
        #{username},#{ctitys},#{ctityss})
    </insert>
</mapper>

